/**
 * 
 */
package testes;

import java.io.InputStream;
import java.util.Scanner;

/**
 * @author juann
 *
 */
public class Teste {

	
	public static void main(String[] args) {
		
		Object testando = new Object();
		
		InputStream arquivo = testando.getClass().getResourceAsStream("/teste.txt");
		
		Scanner scanner = new Scanner(arquivo).useDelimiter("\\A");
		
		String teste = scanner.hasNext() ? scanner.next() : "";
		scanner.close();
		
		System.out.print(teste);

	}

}
